/* 公共 */
page {
  overflow: auto;
  min-height: 100%;
  background-color: #f4f4f4;
  font-family: '微软雅黑';
}
view,text{
  box-sizing: border-box;
  font-family: '微软雅黑';
}
.clearfix:after{content: "";display: block;clear:both;}
.fl{float:left;}
.fr{float:right;}
/* 标题字体 */
.fs-tit{
    font-size:30rpx;
    color:#353535;
}
/* 头部导航 */
.flex-bar {
    padding: 0 0.5rem;
    height: 45px;
    line-height: 45px;
    width: 100%;
    position: relative;
    background-color: #fff;
    box-sizing: border-box;
    box-shadow: 0px 2px 7px 0px rgba(0,0,0,.2);
    margin-bottom:25rpx;
}
.flex-bar .flex-bar-in {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 100%;
  flex-direction: row;
}
.flex-bar .flex-bar-in .on {
    border-bottom: 4px solid #de698c;
}
.flex-bar .flex-bar-in .li {
    width: 25%;
    height: 100%;
    text-align: center;
    box-sizing: border-box;
    margin: 0 5px;
}
.flex-bar .flex-bar-in .a {
    display: block;
    color: #444;
}
